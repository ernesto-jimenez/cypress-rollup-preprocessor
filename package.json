{
  "name": "cypress-rollup-preprocessor",
  "version": "0.2.1",
  "description": "Cypress preprocessor for bundling JavaScript via rollup",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "shx rm -rf dist",
    "build": "tsc",
    "lint": "npm-run-all lint:*",
    "lint:commit": "commitlint --from master",
    "lint:js": "eslint --ext .js,.jsx,.json,.ts,.tsx .",
    "lint:markdown": "markdownlint '**/*.md' --ignore node_modules --ignore CHANGELOG.md",
    "lint:yaml": "yamllint '**/*.{yaml,yml}' --ignore='node_modules/**/*'",
    "release": "npx semantic-release",
    "pretest": "shx rm -rf test/_test-output && shx rm -rf __snapshots__",
    "test": "mocha test/**/*.spec.*"
  },
  "dependencies": {
    "rollup": "^2.26.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.2",
    "@commitlint/config-conventional": "^10.0.0",
    "@cypress/eslint-plugin-dev": "^5.0.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.0.7",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "@types/bluebird": "^3.5.32",
    "@types/chai": "^4.2.12",
    "@types/chai-as-promised": "^7.1.3",
    "@types/fs-extra": "^9.0.1",
    "@types/mocha": "^8.0.2",
    "@types/sinon": "^9.0.4",
    "@types/sinon-chai": "^3.2.4",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/parser": "^3.9.1",
    "bluebird": "^3.7.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^7.7.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-mocha": "^8.0.0",
    "markdownlint-cli": "^0.23.2",
    "mocha": "^8.1.1",
    "mockery": "^2.1.0",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^17.1.1",
    "shx": "^0.3.2",
    "sinon": "^9.0.3",
    "sinon-chai": "^3.5.0",
    "snap-shot-it": "^7.9.3",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.7",
    "yaml-lint": "^1.2.4"
  },
  "peerDependencies": {
    "rollup": "^2.26.3"
  },
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:lmarqs/cypress-rollup-preprocessor.git"
  },
  "homepage": "https://github.com/lmarqs/cypress-rollup-preprocessor",
  "author": {
    "name": "Lucas Marques de Paula",
    "url": "https://github.com/lmarqs"
  },
  "bugs": "https://github.com/lmarqs/cypress-rollup-preprocessor/issues",
  "keywords": [
    "cypress",
    "cypress-plugin",
    "cypress-preprocessor",
    "cypress-rollup-preprocessor",
    "rollup"
  ]
}
